{
	"name": "discord-token-decrypt",
	"version": "0.0.6",
	"description": "A utility for retrieving and decrypting Discord token on macOS.",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"dev": "tsx --watch src/index.ts",
		"build": "rm -rf dist && tsup src/index.ts --format esm --dts",
		"build:watch": "rm -rf dist && tsup src/index.ts --watch --format esm --dts",
		"release": "npm run build && npm publish"
	},
	"files": [
		"dist"
	],
	"os": [
		"darwin"
	],
	"keywords": [
		"discord",
		"electron",
		"cookies",
		"encrypted",
		"curl",
		"decrypt",
		"encrypt",
		"extract"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/lacherogwu/discord-token-decrypt.git"
	},
	"author": "LacheRo`",
	"license": "MIT",
	"type": "module",
	"devDependencies": {
		"@types/node": "^22.13.9",
		"tsup": "^8.4.0",
		"tsx": "^4.19.3",
		"typescript": "~5.8.2"
	},
	"dependencies": {
		"exec-utils": "^0.0.2",
		"level": "^9.0.0"
	}
}
